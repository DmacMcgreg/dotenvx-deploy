{
  "name": "dotenvx-deploy",
  "version": "1.0.2",
  "description": "CLI for managing dotenvx encryption with Vercel deployment and Bitwarden integration",
  "type": "module",
  "bin": {
    "dotenvx-deploy": "./bin/cli.js",
    "dxd": "./bin/cli.js"
  },
  "scripts": {
    "test": "node --test",
    "build": "bun build ./bin/cli.js --compile --outfile dist/dotenvx-deploy",
    "build:all": "npm run build:macos && npm run build:macos-arm && npm run build:linux && npm run build:windows",
    "build:macos": "bun build ./bin/cli.js --compile --target=bun-darwin-x64 --outfile dist/dotenvx-deploy-macos-x64",
    "build:macos-arm": "bun build ./bin/cli.js --compile --target=bun-darwin-arm64 --outfile dist/dotenvx-deploy-macos-arm64",
    "build:linux": "bun build ./bin/cli.js --compile --target=bun-linux-x64 --outfile dist/dotenvx-deploy-linux-x64",
    "build:windows": "bun build ./bin/cli.js --compile --target=bun-windows-x64 --outfile dist/dotenvx-deploy-windows-x64.exe"
  },
  "keywords": [
    "dotenvx",
    "vercel",
    "bitwarden",
    "env",
    "encryption",
    "deployment"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmacMcgreg/dotenvx-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/DmacMcgreg/dotenvx-deploy/issues"
  },
  "homepage": "https://github.com/DmacMcgreg/dotenvx-deploy#readme"
}
